{
  "name": "seothemes/genesis-starter-theme",
  "type": "wordpress-theme",
  "description": "Genesis starter theme with a modern development workflow.",
  "keywords": [
    "WordPress",
    "wordpress-theme-development",
    "genesis-framework"
  ],
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Lee Anthony",
      "homepage": "https://seothemes.com",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/seothemes/genesis-starter-theme/issues",
    "source": "https://github.com/seothemes/genesis-starter-theme"
  },
  "require": {
    "php": ">=5.6",
    "d2/core-asset-loader": "^0.2.0",
    "d2/core-component-boilerplate": "@dev",
    "d2/core-constants": "@dev",
    "d2/core-custom-colors": "@dev",
    "d2/core-genesis-customizer-panels": "^0.2.0",
    "d2/core-genesis-layouts": "^0.1.0",
    "d2/core-genesis-meta-boxes": "^0.1.0",
    "d2/core-genesis-settings": "@dev",
    "d2/core-google-fonts": "@dev",
    "d2/core-hooks": "@dev",
    "d2/core-image-sizes": "@dev",
    "d2/core-page-templates": "^0.1.0",
    "d2/core-plugin-activator": "@dev",
    "d2/core-simple-social-icons": "@dev",
    "d2/core-textdomain": "^0.1.0",
    "d2/core-theme-support": "^0.1.0",
    "d2/core-widget-areas": "^0.1.0"
  },
  "require-dev": {},
  "repositories": [
    {
      "type": "path",
      "url": "temp/d2/core-component-boilerplate",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "temp/d2/core-constants",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "temp/d2/core-custom-colors",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "temp/d2/core-genesis-settings",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "temp/d2/core-google-fonts",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "temp/d2/core-hooks",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "temp/d2/core-image-sizes",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "temp/d2/core-plugin-activator",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "temp/d2/core-simple-social-icons",
      "options": {
        "symlink": true
      }
    }
  ],
  "autoload": {
    "psr-4": {
      "SEOThemes\\GenesisStarterTheme\\": "app/"
    }
  },
  "suggest": {},
  "config": {
    "sort-order": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-create-project-cmd": [
      "yarn install"
    ],
    "post-install-cmd": [
      "yarn install"
    ],
    "stylelint": [
      "stylelint './assets/scss/**/*.scss' --fix"
    ],
    "phpcs": [
      "phpcs"
    ]
  }
}
